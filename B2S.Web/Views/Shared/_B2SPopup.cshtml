@{
    Layout = null;
}
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
    <h4 class="modal-title">Add / Edit Data</h4>
</div>
<form asp-action="Create" onsubmit="return SubmitAddEdit(this)">
    <div class="box-body">
        @RenderBody()
    </div>

    <div class="box-footer">
        <button type="submit" class="btn btn-primary pull-right"><i class="fa fa-save"></i> Submit</button>
        <button type="button" class="btn btn-default pull-left" data-dismiss="modal">Cancel</button>
    </div>
</form>

@RenderSection("Styles", required: false)

@await Html.PartialAsync("_B2SPopupScripts")

@RenderSection("Scripts", required: false)








